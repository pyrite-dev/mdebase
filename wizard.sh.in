#!/bin/sh
mkdir -p "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/mdm"
mkdir -p "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/milkwm"
mkdir -p "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/mpanel"

cp "@CMAKE_CURRENT_BINARY_DIR@/mdm/mdmrc" "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/mdm/"
cp "@CMAKE_CURRENT_BINARY_DIR@/milkwm/milkwmrc" "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/milkwm/"
cp "@CMAKE_CURRENT_BINARY_DIR@/mpanel/mpanelrc" "${DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@/mpanel/"
